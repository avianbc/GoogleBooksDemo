<form class="form-signin" ng-submit="user.logIn()">
    <h2 class="form-signin-heading">Log in</h2>

    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" class="form-control" ng-model="user.username" placeholder="Email" required autofocus>

    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" class="form-control" ng-model="user.password" placeholder="Password" required>

    <button class="btn btn-lg btn-primary btn-block" type="submit">Submit</button>
</form>

